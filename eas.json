{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "default"
      }
    },
    "testflight-debug": {
      "extends": "production",
      "channel": "testflight",
      "env": {
        "APP_ENV": "testflight",
        "EXPO_DEBUG": "true"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6748283968"
      }
    }
  }
}



