{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "types": [
      "nativewind/types"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "app/_layout.jsx",
    "nativewind-env.d.ts",
    "app/hotel/[id].jsx",
    "components/travelersDropdown/TravelersDropdown.jsx",
    "app/store/trpiSearchZustandStore.tsx",
    "components/datePickerModal/datePickerModal.tsx"
  ]
}
