{
  "expo": {
    "name": "Hotfoot AI",
    "slug": "hotfoot-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ai.hotfoot.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["fetch"],
        "NSLocationWhenInUseUsageDescription": "This app uses your location to provide better travel recommendations",
        "NSPhotoLibraryUsageDescription": "This app uses photos to enhance your trip planning experience"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ai.hotfoot.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ecbcdfe0-56cb-4c58-a349-f77cadce5f17"
      }
    },
    "owner": "hotfoot-ai",
     "jsEngine": "jsc"
  }
}
